<p id="notice"><%= notice %></p>

<h1>Participants</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>User</th>
      <th colspan="3">Action</th>
    </tr>
  </thead>

  <tbody>
    <% @participants.each do |participant| %>
      <tr>
        <td><%= participant.event_id %></td>
        <td><%= participant.user_id%></td>
        <td><%= link_to 'Show', participant %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
